{
  "rewrites": [
    {
      "source": "/cardioteam-dec-2025-report",
      "destination": "/cardioteam-dec-2025-report.html"
    },
    {
      "source": "/cardioteam-annual-2025-vs-2024-report",
      "destination": "/cardioteam-annual-2025-vs-2024-report.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
